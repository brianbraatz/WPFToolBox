class=3Dcode-keyword>&gt;</SPAN>
        <SPAN class=3Dcode-keyword>&lt;</SPAN><SPAN =
class=3Dcode-keyword>/</SPAN><SPAN =
class=3Dcode-leadattribute>Grid</SPAN><SPAN =
class=3Dcode-keyword>&gt;</SPAN>

        <SPAN class=3Dcode-keyword>&lt;</SPAN><SPAN =
class=3Dcode-leadattribute>ControlTemplate.Triggers</SPAN><SPAN =
class=3Dcode-keyword>&gt;</SPAN>
          <SPAN class=3Dcode-comment>&lt;!--</SPAN><SPAN =
class=3Dcode-keyword><SPAN class=3Dcode-comment>When the item is =
selected in the TreeView, use the
              "selected" colors and give it a drop shadow. =
</SPAN>--&gt;</SPAN>
          <SPAN class=3Dcode-keyword>&lt;</SPAN><SPAN =
class=3Dcode-leadattribute>Trigger</SPAN> <SPAN =
class=3Dcode-attribute>Property</SPAN><SPAN =
class=3Dcode-keyword>=3D"</SPAN><SPAN =
class=3Dcode-keyword>IsSelected"</SPAN> <SPAN =
class=3Dcode-attribute>Value</SPAN><SPAN =
class=3Dcode-keyword>=3D"</SPAN><SPAN =
class=3Dcode-keyword>True"</SPAN><SPAN class=3Dcode-keyword>&gt;</SPAN>
            <SPAN class=3Dcode-keyword>&lt;</SPAN><SPAN =
