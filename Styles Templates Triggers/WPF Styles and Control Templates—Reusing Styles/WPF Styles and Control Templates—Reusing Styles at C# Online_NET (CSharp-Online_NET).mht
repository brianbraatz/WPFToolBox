#000066">Property</SPAN>=3D<SPAN style=3D"COLOR: =
#ff0000">"Button.IsCancel"</SPAN> <SPAN style=3D"COLOR: =
#000066">Value</SPAN>=3D<SPAN style=3D"COLOR: #ff0000">"False"</SPAN> =
