    <SPAN style=3D"COLOR: blue">private void </SPAN>FetchFriend(<SPAN =
style=3D"COLOR: blue">object </SPAN>state)
    {
        <SPAN style=3D"COLOR: #2b91af">User </SPAN>u =3D =
_service.GetUserInfo(_ID)[0];
       =20
        <SPAN style=3D"COLOR: blue">if </SPAN>( u !=3D <SPAN =
style=3D"COLOR: blue">null </SPAN>)
        {
            <SPAN style=3D"COLOR: =
blue">this</SPAN>._dispatcher.BeginInvoke(<SPAN style=3D"COLOR: =
#2b91af">DispatcherPriority</SPAN>.ApplicationIdle,
                <SPAN style=3D"COLOR: blue">new </SPAN><SPAN =
style=3D"COLOR: #2b91af">ThreadStart</SPAN>(<SPAN style=3D"COLOR: =
blue">delegate
                </SPAN>{
