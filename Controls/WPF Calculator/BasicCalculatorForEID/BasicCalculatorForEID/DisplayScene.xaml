		<Viewbox x:Name="DisplayViewbox" Width="Auto" Height="Auto" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Grid.Row="1" Grid.RowSpan="1" Grid.Column="1" Grid.ColumnSpan="1" MinWidth="0" MinHeight="0" ClipToBounds="True" Margin="3,0,3,4" RenderTransformOrigin="0.5,0.5">
			<StackPanel x:Name="DisplayStackPanel" HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal" RenderTransformOrigin="0.5,0.5">
				<Viewbox Height="Auto" x:Name="DisplayTextViewbox" RenderTransformOrigin="0.5,0.5">
					<TextBox MouseLeftButtonDown="DragAttempt" x:Name="DisplayTextBox" Focusable="False" FontSize="13" Style="{DynamicResource DisplayTextBoxStyle}" Text="Temp" Width="150" Height="18" ClipToBounds="True" Padding="10,1,10,0" Background="{DynamicResource DisplayVisBrush}" BorderThickness="0,0,0,0"/>
				</Viewbox>
			</StackPanel>
		</Viewbox>

	</Grid>

</Window>
