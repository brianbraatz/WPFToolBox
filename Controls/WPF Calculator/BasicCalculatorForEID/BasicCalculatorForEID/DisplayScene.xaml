<Window
	xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
	xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
	xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	xmlns:d="http://schemas.microsoft.com/expression/interactivedesigner/2006"
	mc:Ignorable="d"
	x:Name="RootWindow"
	x:Class="RabidCalc.DisplayCalcScene"
	WindowStyle="None"
	Padding="0,0,0,0">

	<Window.Resources>


	<Style x:Key="TitleHighlightStyle" TargetType="{x:Type Shape}">
		<Setter Property="Fill">
			<Setter.Value>
				<LinearGradientBrush StartPoint="0.5, 0" EndPoint="0.5, 1">
					<LinearGradientBrush.GradientStops>
						<GradientStopCollection>
							<GradientStop Offset="0" Color="#A8FFFFFF"/>
							<GradientStop Offset="0.45" Color="#4CA8A8A8"/>
							<GradientStop Offset="0.65" Color="#00A8A8A8"/>
							<GradientStop Offset="1" Color="#00A8A8A8"/>
						</GradientStopCollection>
					</LinearGradientBrush.GradientStops>
				</LinearGradientBrush>
			</Setter.Value>
		</Setter>
		<Setter Property="OpacityMask">
			<Setter.Value>
				<LinearGradientBrush StartPoint="0,0.5" EndPoint="1,0.5">
					<LinearGradientBrush.GradientStops>
						<GradientStopCollection>
							<GradientStop Offset="0" Color="#CCFFFFFF"/>
							<GradientStop Offset="0.5" Color="#FFFFFFFF"/>
							<GradientStop Offset="1" Color="#CCFFFFFF"/>
						</GradientStopCollection>
					</LinearGradientBrush.GradientStops>
				</LinearGradientBrush>
			</Setter.Value>
		</Setter>
	</Style>

	<Style x:Key="FrameHighlightStyle" TargetType="{x:Type Shape}">
		<Setter Property="Fill">
			<Setter.Value>
				<LinearGradientBrush StartPoint="0.5, 0" EndPoint="0.5, 1">
					<LinearGradientBrush.GradientStops>
						<GradientStopCollection>
							<GradientStop Offset="0" Color="#BBFFFFFF"/>
							<GradientStop Offset="0.4" Color="#4CA8A8A8"/>
							<GradientStop Offset="0.45" Color="#11A8A8A8"/>
							<GradientStop Offset="1" Color="#00A8A8A8"/>
						</GradientStopCollection>
					</LinearGradientBrush.GradientStops>
				</LinearGradientBrush>
			</Setter.Value>
		</Setter>
		<Setter Property="OpacityMask">
			<Setter.Value>
				<LinearGradientBrush StartPoint="0,0.5" EndPoint="1,0.5">
					<LinearGradientBrush.GradientStops>
						<GradientStopCollection>
							<GradientStop Offset="0" Color="#CCFFFFFF"/>
							<GradientStop Offset="0.5" Color="#FFFFFFFF"/>
							<GradientStop Offset="1" Color="#CCFFFFFF"/>
						</GradientStopCollection>
					</LinearGradientBrush.GradientStops>
				</LinearGradientBrush>
			</Setter.Value>
		</Setter>
	</Style>

	<Style x:Key="Framebase" TargetType="{x:Type Shape}">
		<Setter Property="Fill">
			<Setter.Value>
				<LinearGradientBrush StartPoint="0.5, 0" EndPoint="0.5, 1">
					<LinearGradientBrush.GradientStops>
						<GradientStopCollection>
							<GradientStop Color="#FF080008" Offset="0"/>
							<GradientStop Color="#FF110022" Offset="0.2"/>
							<GradientStop Color="#FF331144" Offset="1"/>
						</GradientStopCollection>
					</LinearGradientBrush.GradientStops>
				</LinearGradientBrush>
			</Setter.Value>
		</Setter>
	</Style>

	<Style x:Key="ButtonBox" TargetType="{x:Type Viewbox}">
		<Setter Property="Grid.RowSpan" Value="1"/>
		<Setter Property="Grid.ColumnSpan" Value="1"/>
		<Setter Property="Width" Value="Auto"/>
		<Setter Property="Height" Value="Auto"/>
		<Setter Property="HorizontalAlignment" Value="Center"/>
		<Setter Property="VerticalAlignment" Value="Center"/>
		<Setter Property="Stretch" Value="Uniform"/>
		<Setter Property="Margin" Value="0,0,0,0"/>
		<Setter Property="RenderTransformOrigin" Value="0.5,0.5"/>
	</Style>

	<Style x:Key="DisplayTextBoxStyle" TargetType="{x:Type TextBox}">
		<Setter Property="FontFamily" Value="Trebuchet MS" />
		<Setter Property="FontWeight" Value="Bold" />
		<Setter Property="FontSize" Value="13.5" />
		<Setter Property="MaxLines" Value="1" />
		<Setter Property="TextAlignment" Value="Right" />
		<Setter Property="HorizontalContentAlignment" Value="Right" />
		<Setter Property="Padding" Value="0,1,2,0" />
		<Setter Property="RenderTransformOrigin" Value="0.5,0.5" />
		<Setter Property="Background" Value="Transparent"/>
			<Setter Property="ForceCursor" Value="True" />
			<Setter Property="Cursor" Value="Arrow" />
		<Setter Property="Foreground">
		<Setter.Value>
						<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
					<LinearGradientBrush.GradientStops>
						<GradientStopCollection>
							<GradientStop Color="#EEEEAAAA" Offset="0"/>
							<GradientStop Color="#EEFFBBBB" Offset="1"/>
						</GradientStopCollection>
					</LinearGradientBrush.GradientStops>
				</LinearGradientBrush>
				</Setter.Value>
				</Setter>
	</Style>

	<Style x:Key="WindowBtnStyle" TargetType="{x:Type Button}">
		<Setter Property="OverridesDefaultStyle" Value="true" />
		<Setter Property="Cursor" Value="Hand" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="BorderThickness" Value="0" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="Padding" Value="0" />
		<Setter Property="FontFamily" Value="Trebuchet MS" />
		<Setter Property="FontSize" Value="8pt" />
		<Setter Property="FontStyle" Value="Normal" />
		<Setter Property="FontWeight" Value="Bold" />
		<Setter Property="Foreground">
		<Setter.Value>
			<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
				<LinearGradientBrush.GradientStops>
					<GradientStopCollection>
						<GradientStop Color="#EEEEAAAA" Offset="0"/>
						<GradientStop Color="#EEFFBBBB" Offset="1"/>
					</GradientStopCollection>
				</LinearGradientBrush.GradientStops>
			</LinearGradientBrush>
		</Setter.Value>
		</Setter>
		<Setter Property="Focusable" Value="False" />
	</Style>

	<Style x:Key="FuncBtnStyle" TargetType="{x:Type Button}">
		<Setter Property="OverridesDefaultStyle" Value="True" />
		<Setter Property="Cursor" Value="Hand" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="BorderThickness" Value="0" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="Padding" Value="0" />
		<Setter Property="Margin" Value="4,4,4,4" />
		<Setter Property="RenderTransformOrigin" Value="0.5,0.5" />
		<Setter Property="FontFamily" Value="Trebuchet MS" />
		<Setter Property="FontSize" Value="8pt" />
		<Setter Property="FontStyle" Value="Normal" />
		<Setter Property="FontWeight" Value="Bold" />
		<Setter Property="Foreground">
			<Setter.Value>
				<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
					<LinearGradientBrush.GradientStops>
						<GradientStopCollection>
							<GradientStop Color="#CCCC8888" Offset="0"/>
							<GradientStop Color="#CCEEBBBB" Offset="1"/>
						</GradientStopCollection>
					</LinearGradientBrush.GradientStops>
				</LinearGradientBrush>
			</Setter.Value>
		</Setter>
		<Setter Property="Focusable" Value="False" />
	</Style>

	<Style x:Key="SquareBtnStyle" TargetType="{x:Type Button}">
		<Setter Property="Cursor" Value="Hand" />
		<Setter Property="OverridesDefaultStyle" Value="True" />
		<Setter Property="Background" Value="Transparent" />
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="BorderThickness" Value="0" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="Padding" Value="0" />
		<Setter Property="Margin" Value="2,2,2,2" />
		<Setter Property="RenderTransformOrigin" Value="0.5,0.5" />
		<Setter Property="FontFamily" Value="Trebuchet MS" />
		<Setter Property="FontSize" Value="8pt" />
		<Setter Property="FontStyle" Value="Normal" />
		<Setter Property="FontWeight" Value="Bold" />
		<Setter Property="Foreground">
			<Setter.Value>
				<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
					<LinearGradientBrush.GradientStops>
						<GradientStopCollection>
							<GradientStop Color="#CC8888CC" Offset="0"/>
							<GradientStop Color="#CCBBBBEE" Offset="1"/>

						</GradientStopCollection>
					</LinearGradientBrush.GradientStops>
				</LinearGradientBrush>
			</Setter.Value>
		</Setter>
		<Setter Property="Focusable" Value="False" />
	</Style>

	<ControlTemplate x:Key="CloseBtnTemplate" TargetType="{x:Type Button}">
		<Canvas Name="BtnCanvas" Width="32" Height="12">

			<Rectangle Name="BaseRect" RadiusX="0" RadiusY="0" Width="32" Height="12" Canvas.Left="0" Canvas.Top="0">
				<Rectangle.Fill>
					<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
						<LinearGradientBrush.GradientStops>
							<GradientStopCollection>

								<GradientStop Color="#FF000000" Offset="0"/>
								<GradientStop Color="#FFFF0000" Offset="1"/>
							</GradientStopCollection>
						</LinearGradientBrush.GradientStops>
					</LinearGradientBrush>
				</Rectangle.Fill>
			</Rectangle>

			<Rectangle Name="CausticRect" RadiusX="0" RadiusY="0" Width="32" Height="12" Canvas.Left="0" Canvas.Top="0">
				<Rectangle.Fill>
					<RadialGradientBrush RadiusX="0.5" RadiusY="0.55" GradientOrigin="0.5, 0.15">
						<RadialGradientBrush.GradientStops>
							<GradientStopCollection>
								<GradientStop Color="#00FFFFFF" Offset="0"/>
								<GradientStop Color="#00FFFFFF" Offset="0.66"/>
								<GradientStop Color="#44444444" Offset="0.70"/>
								<GradientStop Color="#D8181818" Offset="1"/>
							</GradientStopCollection>
						</RadialGradientBrush.GradientStops>
					</RadialGradientBrush>
				</Rectangle.Fill>
				<Rectangle.OpacityMask>
					<LinearGradientBrush StartPoint="0.5, 0" EndPoint="0.5, 1">
						<LinearGradientBrush.GradientStops>
							<GradientStopCollection>
								<GradientStop Color="#00000000" Offset="0"/>
								<GradientStop Color="#44000000" Offset="0.5"/>
								<GradientStop Color="#FFFFFFFF" Offset="1"/>
							</GradientStopCollection>
						</LinearGradientBrush.GradientStops>
					</LinearGradientBrush>
				</Rectangle.OpacityMask>
			</Rectangle>

			<Viewbox Name="ContentViewbox" Width="26" Height="10" Stretch="Uniform" Canvas.Left="3" Canvas.Top="1">
				<ContentPresenter Name="Content"/>
			</Viewbox>

			<Rectangle Name="HighlightRect" RadiusX="0" RadiusY="0" Width="30" Height="11" Canvas.Left="1" Canvas.Top="0.5" >
				<Rectangle.Fill>
					<LinearGradientBrush StartPoint="0.5, 0" EndPoint="0.5, 1">
						<LinearGradientBrush.GradientStops>
							<GradientStopCollection>
								<GradientStop Color="#A8FFFFFF" Offset="0"/>
								<GradientStop Color="#4CA8A8A8" Offset="0.45"/>
								<GradientStop Color="#00A8A8A8" Offset="0.65"/>
								<GradientStop Color="#00A8A8A8" Offset="1"/>
							</GradientStopCollection>
						</LinearGradientBrush.GradientStops>
					</LinearGradientBrush>
				</Rectangle.Fill>
				<Rectangle.OpacityMask>
					<LinearGradientBrush StartPoint="0,0.5" EndPoint="1,0.5">
						<LinearGradientBrush.GradientStops>
							<GradientStopCollection>
								<GradientStop Color="#CCFFFFFF" Offset="0"/>
								<GradientStop Color="#FFFFFFFF" Offset="0.5"/>
								<GradientStop Color="#CCFFFFFF" Offset="1"/>
							</GradientStopCollection>
						</LinearGradientBrush.GradientStops>
					</LinearGradientBrush>
				</Rectangle.OpacityMask>
			</Rectangle>

		</Canvas>
		<ControlTemplate.Triggers>

			<Trigger Property="IsMouseOver" Value="true">
				<Setter TargetName="CausticRect" Property="Fill">
					<Setter.Value>
						<RadialGradientBrush RadiusX="0.5" RadiusY="0.55" GradientOrigin="0.5, 0.15">
							<RadialGradientBrush.GradientStops>
								<GradientStopCollection>
									<GradientStop Color="#00FFFFFF" Offset="0"/>
									<GradientStop Color="#00FFFFFF" Offset="0.7"/>
									<GradientStop Color="#44444444" Offset="0.8"/>
									<GradientStop Color="#A8222222" Offset="1"/>
								</GradientStopCollection>
							</RadialGradientBrush.GradientStops>
						</RadialGradientBrush>
					</Setter.Value>
				</Setter>
				<Setter TargetName="CausticRect" Property="OpacityMask">
					<Setter.Value>
						<LinearGradientBrush StartPoint="0.5, 0" EndPoint="0.5, 1">
							<LinearGradientBrush.GradientStops>
								<GradientStopCollection>
									<GradientStop Color="#00000000" Offset="0"/>
									<GradientStop Color="#44666666" Offset="0.5"/>
									<GradientStop Color="#FFFFFFFF" Offset="1"/>
								</GradientStopCollection>
							</LinearGradientBrush.GradientStops>
						</LinearGradientBrush>
					</Setter.Value>
				</Setter>
				<Setter TargetName="BaseRect" Property="Fill">
					<Setter.Value>
						<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
							<LinearGradientBrush.GradientStops>
								<GradientStopCollection>
									<GradientStop Color="#FF4C0000" Offset="0"/>
									<GradientStop Color="#FFFF0000" Offset="1"/>
								</GradientStopCollection>
							</LinearGradientBrush.GradientStops>
						</LinearGradientBrush>
					</Setter.Value>
				</Setter>
				<Setter Property="Foreground">
					<Setter.Value>
						<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
							<LinearGradientBrush.GradientStops>
								<GradientStopCollection>
									<GradientStop Color="#EEAAAAEE" Offset="0"/>
									<GradientStop Color="#EECCCCFF" Offset="1"/>
								</GradientStopCollection>
							</LinearGradientBrush.GradientStops>
						</LinearGradientBrush>
					</Setter.Value>
				</Setter>
			</Trigger>

		</ControlTemplate.Triggers>
	</ControlTemplate>

	<ControlTemplate x:Key="WideBtnTemplate" TargetType="{x:Type Button}">

		<Viewbox Stretch="Fill" Name="WideBtnViewbox" RenderTransformOrigin="0.5,0.5">
			<Canvas Width="41" Height="26" Name="WideBtnCanvas">
				<Rectangle Name="BevelRect" RadiusX="4" RadiusY="4" StrokeThickness="4" Width="41" Height="26" Canvas.Left="0" Canvas.Top="0">
					<Rectangle.Stroke>
						<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
							<LinearGradientBrush.GradientStops>
								<GradientStopCollection>
									<GradientStop Color="#FF000000" Offset="0"/>
									<GradientStop Color="#FFFFFFFF" Offset="1"/>
								</GradientStopCollection>
							</LinearGradientBrush.GradientStops>
						</LinearGradientBrush>
					</Rectangle.Stroke>
					<Rectangle.OpacityMask>
						<LinearGradientBrush StartPoint="0,0.5" EndPoint="1,0.5">
							<LinearGradientBrush.GradientStops>
								<GradientStopCollection>
									<GradientStop Color="#22888888" Offset="0"/>
									<GradientStop Color="#44888888" Offset="0.5"/>
									<GradientStop Color="#22888888" Offset="1"/>
								</GradientStopCollection>
							</LinearGradientBrush.GradientStops>
						</LinearGradientBrush>
					</Rectangle.OpacityMask>
				</Rectangle>

				<Rectangle Name="BaseRect" RadiusX="4" RadiusY="4" Width="39" Height="24.5" Canvas.Left="1" Canvas.Top="0.5">
					<Rectangle.Fill>
						<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
							<LinearGradientBrush.GradientStops>
								<GradientStopCollection>
									<GradientStop Color="#FF000000" Offset="0"/>
									<GradientStop Color="#FF0000FF" Offset="1"/>
								</GradientStopCollection>
							</LinearGradientBrush.GradientStops>
						</LinearGradientBrush>
					</Rectangle.Fill>
				</Rectangle>

				<Viewbox Name="ContentViewbox" Width="18" Height="18" Stretch="Uniform" Canvas.Left="11.5" Canvas.Top="4.5">
					<ContentPresenter x:Name="Content"/>
				</Viewbox>

				<Rectangle Name="SideShadow" RadiusX="4" RadiusY="4" Width="39" Height="24.5" Canvas.Left="1" Canvas.Top="0.5">
					<Rectangle.Fill>
						<LinearGradientBrush StartPoint="0,0.5" EndPoint="1,0.5">
							<LinearGradientBrush.GradientStops>
								<GradientStopCollection>
									<GradientStop Color="#CC222222" Offset="0"/>
									<GradientStop Color="#11000000" Offset="0.33"/>
									<GradientStop Color="#11000000" Offset="0.66"/>
									<GradientStop Color="#CC222222" Offset="1"/>
								</GradientStopCollection>
							</LinearGradientBrush.GradientStops>
						</LinearGradientBrush>
					</Rectangle.Fill>
					<Rectangle.OpacityMask>
						<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
							<LinearGradientBrush.GradientStops>
								<GradientStopCollection>
									<GradientStop Color="#00888888" Offset="0"/>
									<GradientStop Color="#77888888" Offset="0.2"/>
									<GradientStop Color="#FF888888" Offset="0.5"/>
									<GradientStop Color="#77888888" Offset="0.8"/>
									<GradientStop Color="#00888888" Offset="1"/>
								</GradientStopCollection>
							</LinearGradientBrush.GradientStops>
						</LinearGradientBrush>
					</Rectangle.OpacityMask>
				</Rectangle>

				<Rectangle Name="ShadowEllipse" RadiusX="4" RadiusY="4" Width="39" Height="24.5" Canvas.Left="1" Canvas.Top="0.5">
					<Rectangle.Fill>
						<RadialGradientBrush RadiusX="0.7" RadiusY="0.7" GradientOrigin="0.5,0">
							<RadialGradientBrush.GradientStops>
								<GradientStopCollection>
									<GradientStop Color="#00FFFFFF" Offset="0"/>
									<GradientStop Color="#00000000" Offset="0.66"/>
									<GradientStop Color="#99000000" Offset="0.9"/>
									<GradientStop Color="#FF000000" Offset="0.1"/>
								</GradientStopCollection>
							</RadialGradientBrush.GradientStops>
						</RadialGradientBrush>
					</Rectangle.Fill>
					<Rectangle.OpacityMask>
						<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
							<LinearGradientBrush.GradientStops>
								<GradientStopCollection>
									<GradientStop Color="#00888888" Offset="0"/>
									<GradientStop Color="#00888888" Offset="0.33"/>
									<GradientStop Color="#4C888888" Offset="0.5"/>
									<GradientStop Color="#EE888888" Offset="1"/>
								</GradientStopCollection>
							</LinearGradientBrush.GradientStops>
						</LinearGradientBrush>
					</Rectangle.OpacityMask>
				</Rectangle>

				<Rectangle Name="HighlightRect" Opacity="1" RadiusX="4" RadiusY="4" Width="38" Height="23.5" Canvas.Left="1.5" Canvas.Top="1.5">
					<Rectangle.Fill>
						<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
							<LinearGradientBrush.GradientStops>
								<GradientStopCollection>
									<GradientStop Color="#22888888" Offset="0"/>
									<GradientStop Color="#BBEEEEEE" Offset="0.05"/>
									<GradientStop Color="#88EEEEEE" Offset="0.30"/>
									<GradientStop Color="#66EEEEEE" Offset="0.60"/>
									<GradientStop Color="#22888888" Offset="0.95"/>
									<GradientStop Color="#00888888" Offset="1"/>
								</GradientStopCollection>
							</LinearGradientBrush.GradientStops>
							<LinearGradientBrush.RelativeTransform>
								<RotateTransform Angle="1"/>
							</LinearGradientBrush.RelativeTransform>
						</LinearGradientBrush>
					</Rectangle.Fill>
					<Rectangle.OpacityMask>
						<LinearGradientBrush StartPoint="0,0.5" EndPoint="1,0.5">
							<LinearGradientBrush.GradientStops>
								<GradientStopCollection>
									<GradientStop Color="#22888888" Offset="0"/>
									<GradientStop Color="#AA888888" Offset="0.05"/>
									<GradientStop Color="#EE888888" Offset="0.5"/>
									<GradientStop Color="#AA888888" Offset="0.95"/>
									<GradientStop Color="#22888888" Offset="1"/>
								</GradientStopCollection>
							</LinearGradientBrush.GradientStops>
						</LinearGradientBrush>
					</Rectangle.OpacityMask>
				</Rectangle>
			</Canvas>
		</Viewbox>

		<ControlTemplate.Triggers>
			<Trigger Property="IsPressed" Value="true">
				<Setter Property="Opacity" TargetName="HighlightRect" Value="0" />
			</Trigger>
			<Trigger Property="IsMouseOver" Value="true">

				<Setter Property="Fill" TargetName="SideShadow">
					<Setter.Value>
						<LinearGradientBrush StartPoint="0,0.5" EndPoint="1,0.5">
							<LinearGradientBrush.GradientStops>
								<GradientStopCollection>
									<GradientStop Color="#99222222" Offset="0"/>
									<GradientStop Color="#11000000" Offset="0.33"/>
									<GradientStop Color="#11000000" Offset="0.66"/>
									<GradientStop Color="#99222222" Offset="1"/>
								</GradientStopCollection>
							</LinearGradientBrush.GradientStops>
						</LinearGradientBrush>
					</Setter.Value>
				</Setter>
				<Setter Property="Fill" TargetName="BaseRect">
					<Setter.Value>
						<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
							<LinearGradientBrush.GradientStops>
								<GradientStopCollection>
									<GradientStop Color="#FF000099" Offset="0"/>
									<GradientStop Color="#FF000066" Offset="0"/>
									<GradientStop Color="#FF0000FF" Offset="1"/>
								</GradientStopCollection>
							</LinearGradientBrush.GradientStops>
						</LinearGradientBrush>
					</Setter.Value>
				</Setter>
				<Setter Property="Foreground">
					<Setter.Value>
						<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
							<LinearGradientBrush.GradientStops>
								<GradientStopCollection>
									<GradientStop Color="#EEAAAAEE" Offset="0"/>
									<GradientStop Color="#EECCCCFF" Offset="1"/>
								</GradientStopCollection>
							</LinearGradientBrush.GradientStops>
						</LinearGradientBrush>
					</Setter.Value>
				</Setter>
			</Trigger>
		</ControlTemplate.Triggers>
	</ControlTemplate>

	<ControlTemplate x:Key="SquareBtnTemplate" TargetType="{x:Type Button}">

		<Viewbox Stretch="Fill" Name="SquareBtnViewbox" RenderTransformOrigin="0.5,0.5">
			<Canvas Width="26" Height="26" Name="SquareBtnCanvas">
				<Rectangle Name="BevelRect" RadiusX="4" RadiusY="4" StrokeThickness="4" Width="26" Height="26" Canvas.Left="0" Canvas.Top="0">
					<Rectangle.Stroke>
						<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
							<LinearGradientBrush.GradientStops>
								<GradientStopCollection>
									<GradientStop Color="#FF000000" Offset="0"/>
									<GradientStop Color="#FFFFFFFF" Offset="1"/>
								</GradientStopCollection>
							</LinearGradientBrush.GradientStops>
						</LinearGradientBrush>
					</Rectangle.Stroke>
					<Rectangle.OpacityMask>
						<LinearGradientBrush StartPoint="0,0.5" EndPoint="1,0.5">
							<LinearGradientBrush.GradientStops>
								<GradientStopCollection>
									<GradientStop Color="#22888888" Offset="0"/>
									<GradientStop Color="#44888888" Offset="0.5"/>
									<GradientStop Color="#22888888" Offset="1"/>
								</GradientStopCollection>
							</LinearGradientBrush.GradientStops>
						</LinearGradientBrush>
					</Rectangle.OpacityMask>
				</Rectangle>

				<Rectangle Name="BaseRect" RadiusX="4" RadiusY="4" Width="22" Height="22.5" Canvas.Left="2" Canvas.Top="2.5">
					<Rectangle.Fill>
						<LinearGradientBrush StartPoint="0.5,0" EndPoint="0.5,1">
							<LinearGradientBrush.GradientStops>
								<GradientStopCollection>
									<GradientStop Color="#FF000000" Offset="0"/>
									<GradientStop Color="#FF0000FF" Offset="1"/>
								</GradientStopCollection>
							</LinearGradientBrush.GradientStops>
						</LinearGradientBrush>
