	<Grid Name="myGrid" Background="Transparent"  >
	
		<Viewport3D Focusable="true" Name="myViewport3D" ClipToBounds="true" >
			<Viewport3D.Camera>
				<PerspectiveCamera 
				Position="0,0,10" 
				LookDirection="0,0,-1"
				UpDirection="0,1,0"
				NearPlaneDistance="0.25"
				FarPlaneDistance="20"
				FieldOfView="60" />
			</Viewport3D.Camera>
			
			<Viewport3D.Children>
			
				<!-- Carousel Model -->
				<ModelVisual3D>
				
					<!-- Trackball uses this transform -->
					<ModelVisual3D.Transform>
						<Transform3DGroup>
							<Transform3DGroup.Children>
								<Transform3DCollection >

									<ScaleTransform3D ScaleX="1" ScaleY="1"  ScaleZ="1" />
									<RotateTransform3D>
										<RotateTransform3D.Rotation>
											<AxisAngleRotation3D Axis="0 1 0" Angle="0" />
										</RotateTransform3D.Rotation>
									</RotateTransform3D>
									<TranslateTransform3D OffsetX="0" OffsetY="0" OffsetZ="0" />
									
								</Transform3DCollection>
							</Transform3DGroup.Children>
						</Transform3DGroup>
					</ModelVisual3D.Transform>

					<ModelVisual3D.Children>
					
						<!-- Child 0 -->
						<ModelVisual3D>
						
							<ModelVisual3D.Transform>
								<Transform3DGroup>
									<Transform3DGroup.Children>
										<Transform3DCollection >

											<ScaleTransform3D ScaleX="1" ScaleY="1"  ScaleZ="1" />
											<RotateTransform3D>
												<RotateTransform3D.Rotation>
													<AxisAngleRotation3D Axis="0 1 0" Angle="0" />
												</RotateTransform3D.Rotation>
											</RotateTransform3D>
											<TranslateTransform3D OffsetX="0" OffsetY="0" OffsetZ="0" />
											
										</Transform3DCollection>
									</Transform3DGroup.Children>
								</Transform3DGroup>
							</ModelVisual3D.Transform>
							
							<ModelVisual3D.Content>
								
								<GeometryModel3D Geometry="{StaticResource PlaneMesh}" >

									<GeometryModel3D.Transform>
										<Transform3DGroup>
											<Transform3DGroup.Children>
												<Transform3DCollection >

													<ScaleTransform3D ScaleX="1" ScaleY=" 0.75"  ScaleZ="1" />
													<RotateTransform3D>
														<RotateTransform3D.Rotation>
															<AxisAngleRotation3D Axis="0 1 0" Angle="0" />
														</RotateTransform3D.Rotation>
													</RotateTransform3D>
													<TranslateTransform3D OffsetX="0" OffsetY="0" OffsetZ="0" />

												</Transform3DCollection>
											</Transform3DGroup.Children>
										</Transform3DGroup>
									</GeometryModel3D.Transform>
								</GeometryModel3D>
								
							</ModelVisual3D.Content>
								
						</ModelVisual3D>
						
						
						<!-- Child 1 -->
						<ModelVisual3D>
						
							<ModelVisual3D.Transform>
								<Transform3DGroup>
									<Transform3DGroup.Children>
										<Transform3DCollection >

											<ScaleTransform3D ScaleX="1" ScaleY="1"  ScaleZ="1" />
											<RotateTransform3D>
												<RotateTransform3D.Rotation>
													<AxisAngleRotation3D Axis="0 1 0" Angle="-45" />
												</RotateTransform3D.Rotation>
											</RotateTransform3D>
											<TranslateTransform3D OffsetX="3.2" OffsetY="-0.1"  OffsetZ="0.8" />
											
										</Transform3DCollection>
									</Transform3DGroup.Children>
								</Transform3DGroup>
							</ModelVisual3D.Transform>
							
							<ModelVisual3D.Content>
								
								<GeometryModel3D Geometry="{StaticResource PlaneMesh}" >

									<GeometryModel3D.Transform>
										<Transform3DGroup>
											<Transform3DGroup.Children>
												<Transform3DCollection >

													<ScaleTransform3D ScaleX="1" ScaleY=" 0.75"  ScaleZ="1" />
													<RotateTransform3D>
														<RotateTransform3D.Rotation>
															<AxisAngleRotation3D Axis="0 1 0" Angle="0" />
														</RotateTransform3D.Rotation>
													</RotateTransform3D>
													<TranslateTransform3D OffsetX="0" OffsetY="0" OffsetZ="0" />

												</Transform3DCollection>
											</Transform3DGroup.Children>
										</Transform3DGroup>
									</GeometryModel3D.Transform>
								</GeometryModel3D>
								
							</ModelVisual3D.Content>
								
						</ModelVisual3D>

						<!-- Child 2 -->
						<ModelVisual3D>
						
							<ModelVisual3D.Transform>
								<Transform3DGroup>
									<Transform3DGroup.Children>
										<Transform3DCollection >

											<ScaleTransform3D ScaleX="1" ScaleY="1"  ScaleZ="1" />
											<RotateTransform3D>
												<RotateTransform3D.Rotation>
													<AxisAngleRotation3D Axis="0 1 0" Angle="-45" />
												</RotateTransform3D.Rotation>
											</RotateTransform3D>
											<TranslateTransform3D OffsetX="2.4" OffsetY="-0.2" OffsetZ="1.6" />
											
										</Transform3DCollection>
									</Transform3DGroup.Children>
								</Transform3DGroup>
							</ModelVisual3D.Transform>
							
							<ModelVisual3D.Content>
								
								<GeometryModel3D Geometry="{StaticResource PlaneMesh}" >

									<GeometryModel3D.Transform>
										<Transform3DGroup>
											<Transform3DGroup.Children>
												<Transform3DCollection >

													<ScaleTransform3D ScaleX="1" ScaleY=" 0.75"  ScaleZ="1" />
													<RotateTransform3D>
														<RotateTransform3D.Rotation>
															<AxisAngleRotation3D Axis="0 1 0" Angle="0" />
														</RotateTransform3D.Rotation>
													</RotateTransform3D>
													<TranslateTransform3D OffsetX="0" OffsetY="0" OffsetZ="0" />

												</Transform3DCollection>
											</Transform3DGroup.Children>
										</Transform3DGroup>
									</GeometryModel3D.Transform>
								</GeometryModel3D>
								
							</ModelVisual3D.Content>
								
						</ModelVisual3D>
	
						<!-- Child 3 -->
						<ModelVisual3D>
						
							<ModelVisual3D.Transform>
								<Transform3DGroup>
									<Transform3DGroup.Children>
										<Transform3DCollection >

											<ScaleTransform3D ScaleX="1" ScaleY="1"  ScaleZ="1" />
											<RotateTransform3D>
												<RotateTransform3D.Rotation>
													<AxisAngleRotation3D Axis="0 1 0" Angle="-45" />
												</RotateTransform3D.Rotation>
											</RotateTransform3D>
											<TranslateTransform3D OffsetX="1.6" OffsetY="-0.3" OffsetZ="2.4" />
											
										</Transform3DCollection>
									</Transform3DGroup.Children>
								</Transform3DGroup>
							</ModelVisual3D.Transform>
							
							<ModelVisual3D.Content>
								
								<GeometryModel3D Geometry="{StaticResource PlaneMesh}" >

									<GeometryModel3D.Transform>
										<Transform3DGroup>
											<Transform3DGroup.Children>
												<Transform3DCollection >

													<ScaleTransform3D ScaleX="1" ScaleY=" 0.75"  ScaleZ="1" />
													<RotateTransform3D>
														<RotateTransform3D.Rotation>
															<AxisAngleRotation3D Axis="0 1 0" Angle="0" />
														</RotateTransform3D.Rotation>
													</RotateTransform3D>
													<TranslateTransform3D OffsetX="0" OffsetY="0" OffsetZ="0" />

												</Transform3DCollection>
											</Transform3DGroup.Children>
										</Transform3DGroup>
									</GeometryModel3D.Transform>
								</GeometryModel3D>
								
							</ModelVisual3D.Content>
								
						</ModelVisual3D>
								
								
						<!-- Child 4 -->
						<ModelVisual3D>
						
							<ModelVisual3D.Transform>
								<Transform3DGroup>
									<Transform3DGroup.Children>
										<Transform3DCollection >

											<ScaleTransform3D ScaleX="1" ScaleY="1"  ScaleZ="1" />
											<RotateTransform3D>
												<RotateTransform3D.Rotation>
													<AxisAngleRotation3D Axis="0 1 0" Angle="-45" />
												</RotateTransform3D.Rotation>
											</RotateTransform3D>
											<TranslateTransform3D OffsetX="0.8" OffsetY="-0.4" OffsetZ="3.2" />
											
										</Transform3DCollection>
									</Transform3DGroup.Children>
								</Transform3DGroup>
