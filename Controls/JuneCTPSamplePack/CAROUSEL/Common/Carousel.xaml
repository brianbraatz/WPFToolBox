				<StaticResource ResourceKey="TranslateXTo0" />
			</ParallelTimeline>
			<ParallelTimeline  Storyboard.TargetName="myViewport3D" Storyboard.TargetProperty="Children[0].Children[8].Transform.Children[2].OffsetY">
