	<Style x:Key="LEDRadBtnLeft" TargetType="{x:Type RadioButton}">
		<Setter Property="OverridesDefaultStyle" Value="true" />
		<Setter Property="Background" Value="Transparent"/>
		<Setter Property="BorderBrush" Value="Transparent"/>
		<Setter Property="FontFamily" Value="Trebuchet MS" />
		<Setter Property="FontSize" Value="9pt" />
		<Setter Property="FontStyle" Value="Normal" />
		<Setter Property="FontWeight" Value="Normal" />
		<Setter Property="Foreground" Value="Black" />
		<Setter Property="Padding" Value="0,0,0,0" />
		<Setter Property="Margin" Value="2,2,0,2" />
		<Setter Property="Focusable" Value="False" />
		<Setter Property="Template">
			<Setter.Value>
				<ControlTemplate TargetType="{x:Type RadioButton}">
					<BulletDecorator Background="Transparent">
					<Grid>
							<Grid.ColumnDefinitions>
			<ColumnDefinition />
			<ColumnDefinition />

		</Grid.ColumnDefinitions>
		<Grid.RowDefinitions>

			<RowDefinition />

		</Grid.RowDefinitions>
						<Canvas Width="12" Height="12" Grid.Row="0" Grid.Column="0" Grid.RowSpan="1" Grid.ColumnSpan="1" Name="RadBtnCircles" RenderTransformOrigin="0.5,0.5">
							<Ellipse Style="{DynamicResource DisplayBevelStyle}" Width="12" Height="12" Canvas.Left="0" Canvas.Top="0" Name="OuterBevel" RenderTransformOrigin="0.5,0.5"/>
							<Ellipse Style="{DynamicResource MainFrameStyle}" Width="11" Height="11" Canvas.Left="0.5" Canvas.Top="0.5" Name="OuterFace" RenderTransformOrigin="0.5,0.5"/>
							<Ellipse Style="{DynamicResource InvertBtnHighlightStyle}" Width="11" Height="11" Canvas.Left="0.5" Canvas.Top="0.5" x:Name="OuterHighlight" RenderTransformOrigin="0.5,0.5"/>
							<Canvas Width="8" Height="8" Canvas.Left="2" Canvas.Top="2" Name="InnerCircs" RenderTransformOrigin="0.5,0.5">
								<Ellipse Style="{DynamicResource MainBevelStyle}" Width="8" Height="8" Canvas.Left="0" Canvas.Top="0" Name="InnerBevel" RenderTransformOrigin="0.5,0.5"/>
								<Ellipse Style="{DynamicResource DisplayFaceStyle}" Width="7" Height="7" Canvas.Left="0.5" Canvas.Top="0.5" Name="InnerFace" RenderTransformOrigin="0.5,0.5"/>
								<Ellipse Style="{DynamicResource ButtonHighlightStyle}" Width="7" Height="7" Canvas.Left="0.5" Canvas.Top="0.5" Name="InnerHiglight" RenderTransformOrigin="0.5,0.5"/>
							</Canvas>
						</Canvas>
						<Canvas Margin="3,0,0,0" Width="52" Height="12" Grid.Row="0" Grid.Column="1" Grid.RowSpan="1" Grid.ColumnSpan="1" Name="RadBtnContentCanvas" RenderTransformOrigin="0.5,0.5">
							<Rectangle RadiusX="6" RadiusY="6" Style="{DynamicResource DisplayFaceStyle}" Width="51" Height="11" Canvas.Left="0.5" Canvas.Top="0.5" x:Name="ContFace" RenderTransformOrigin="0.5,0.5"/>
							<ContentPresenter Name="RadBtnContentPresenter" RecognizesAccessKey="True"/>
							<Rectangle RadiusX="6" RadiusY="6" Opacity="0.8" Style="{DynamicResource ButtonHighlightStyle}" Width="51" Height="11" Canvas.Left="0.5" Canvas.Top="0.5" Name="ContHighlight" RenderTransformOrigin="0.5,0.5"/>
							<Rectangle RadiusX="6" RadiusY="6" StrokeThickness="1" Style="{DynamicResource MainBevelStyle}" Width="52" Height="12" Canvas.Left="0" Canvas.Top="0" Name="ContBevel" RenderTransformOrigin="0.5,0.5"/>
						</Canvas>
						</Grid>
					</BulletDecorator>
					<ControlTemplate.Triggers>
						<Trigger Property="IsChecked" Value="false">
							<Setter TargetName="InnerCircs" Property="Visibility" Value="Collapsed" />
						</Trigger>
						<Trigger Property="IsPressed" Value="true">
							<Setter TargetName="InnerCircs" Property="Opacity" Value="0.66" />
							<Setter TargetName="ContHighlight" Property="Opacity" Value="0"/>
							<Setter TargetName="OuterHighlight" Property="Opacity" Value="0"/>
							<Setter TargetName="OuterFace" Property="Style" Value="{DynamicResource DisplayFaceStyle}"/>
							<Setter TargetName="InnerFace" Property="Style" Value="{DynamicResource RedShapeStyle}"/>
						</Trigger>
					</ControlTemplate.Triggers>
				</ControlTemplate>
			</Setter.Value>
		</Setter>
	</Style>

