	var variables=3DRTE_GetEditorInstanceVariables(strBaseElementID);
	if (variables !=3Dnull && variables.overrides.GetToolBarDefinition =
!=3Dnull)
	{
		return variables.overrides.GetToolBarDefinition(strBaseElementID, =
strWebLanguage);
	}
	else if (variables !=3Dnull)
	{
		return RTE_GetCompatibleToolBarDefinition(
			strWebLanguage,
			variables.aSettings.fRestrictedMode,
			variables.aSettings.fAllowHyperlink,
			variables.aSettings.fIsVisible);
	}
	else
	{
		return null;
	}
}
